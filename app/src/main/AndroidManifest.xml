<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.isabel.examen_vinted">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Examen_Vinted"
        android:usesCleartextTraffic="true">
        <activity android:name=".splashScreen.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".usuarios.login.LoginUsuarioView"/>
        <activity android:name=".usuarios.registro.RegistroUsuarioView" />
        <activity android:name=".productos.categoriaProductos.CategoriaProductoView" />
        <activity android:name=".productos.listaProductos.DetalleProductosView" />
        <activity android:name=".MainActivity">
        </activity>
    </application>
</manifest>